
# Example .env for AI Task Breakdown Assistant (server)
# Copy this file to .env and fill in your values

# Port for Express server
PORT=3001

# AI provider (currently only 'gemini' is supported)
AI_PROVIDER=gemini

# Your Gemini API key (get from Google AI Console)
AI_API_KEY=

# Gemini model (e.g. gemini-2.5-flash or gemini-1.5-pro)
AI_MODEL=gemini-2.5-flash

# Set to 'production' for deployment
NODE_ENV=production
